import java.util.*;
public class Solution {
	public static void main(String[] args) {
		int arr[]=new int[] {1,2,3,4,6,7,8};
		int x = 5;
        int l=0,r=arr.length -1, fl=0;
        while(l<=r){
            int m = l+(r-l)/2;
            if(arr[m]==x){
                fl=1;break;
            }
            if(arr[m]<x)
                l=m+1;
            else
                r=m-1;
                
        }
        return;
	}
}
